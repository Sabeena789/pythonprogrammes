
#Creating Variables
x = 5
y = "John"
print(x)
print(y)

x = 4       
x = "Sally" 
print(x)


x = 5
y = "John"
print(type(x))
print(type(y))


x = "John"
# is the same as
x = 'John'

x, y, z = "Orange", "Banana", "Cherry"
print(x)
print(y)
print(z)

#One Value to Multiple Variables
x = y = z = "Orange"
print(x)
print(y)
print(z)

#Unpack a Collection

fruits = ["apple", "banana", "cherry"]
x, y, z = fruits
print(x)
print(y)
print(z)
